<!DOCTYPE html>
<html>

<head>
  <%- include('../../layout/head') %>
</head>

<body>
    <header>
        <%- include('../../layout/header') %>
    </header>
  <main>
    <!-- sub navigation -->

    <ul class="subnav">
        <li>
          <i class="fas fa-search"></i>
          <a href="/new">Search</a>
        </li>
        <li>
            <i class="fas fa-book-open"></i>
          <a href="/">Shelf</a>
        </li>
      </ul>
      <!-- end subnav -->
  <h1><%= searchResults.length %> Book Results</h1>

  <ul>
    <% searchResults.forEach(volumeInfo => { %>
  <div>
    <li class="img"><img src='<%= volumeInfo.imageLinks %>' /></li>
    <li class="title"><h4><%= volumeInfo.title %></h4></li>
    <li class="author"><%= volumeInfo.authors %></li>
    <li class="description"><%= volumeInfo.description %></li>
  </div>
    <% }); %>

  </ul>
</main>
<footer> 
    <%- include('../../layout/footer') %>
</footer>
</body>

</html>